DATABASE_URL="file:./prisma/dev.db"

# NextAuth (obrigatório para login)
# Gerar com: openssl rand -base64 32
AUTH_SECRET=

# Puppeteer: evita download do Chromium no "npm install" (use se já tiver Chrome instalado)
